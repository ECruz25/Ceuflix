{
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": ["eslint:recommended", "google"],
  "rules": {
    "linebreak-style": "off",
    "no-console": 0,
    "comma-dangle": 1,
    "no-unused-vars": 1
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 8
  },
  "plugins": ["html"]
}
